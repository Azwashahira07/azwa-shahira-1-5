#include <stdio.h>

int main() {
    int score;
    printf("Enter score: ");
    scanf("%d", &score);

    if(score >= 80){
        printf("Grade A");
    }
    else if(score >= 60){
        printf("Grade B");
    }
    else if(score >= 50){
        printf("Grade C");
    }
    else{
        printf("Fail");
    }

    return 0;
}
